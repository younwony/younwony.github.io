<section class="section">
    <h2 class="section-title">Core Competencies</h2>
    <div class="competency-grid">
        <% competencies.forEach(function(comp) { %>
        <div class="competency-item">
            <strong><%= comp.title %></strong>
            <span><%= comp.description %></span>
        </div>
        <% }); %>
    </div>
</section>
