<section class="section">
    <h2 class="section-title">Technical Skills</h2>
    <div class="skills-container">
        <% skills.forEach(function(skill) { %>
        <div class="skill-category">
            <strong><%= skill.category %></strong>
            <span><%= skill.items %></span>
        </div>
        <% }); %>
    </div>
</section>
